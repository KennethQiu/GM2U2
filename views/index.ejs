<%- include('layout/header') %>
<div class="container">
  <div class="row">
    <div class="col-md-2">
      <ul class="modules">
        <li>Time</li>
        <li>Weather</li>
        <li>Traffic</li>
        <li>News</li>
        <button> Add to board</button>
      </ul>
    </div>
    <div class="col-md-10">
      <div class="boards">
        <button id="add_board">Add a board</button>
        <h1>Bulletin board</h1>
        <div class="board">
          <div class="board_title">
            <h3>Board 1</h3>
            <button>Play</button>
          </div>
          <div class="board_body row">
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
 <ul class="boards">
  <% for(var i=0; i<boards.length; i++) {%>
    <li class="quote">
      <span><%= boards[i].name %></span>
    </li>
  <% } %>
  </ul>



<%- include('layout/footer') %>
